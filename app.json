{
  "expo": {
    "name": "Ferramentas Diárias",
    "slug": "Ferramentas Diárias",
    "version": "2.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1C304A"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#1C304A"
      },
      "config": {
        "googleMobileAdsAppId": ""
      },
      "package": "com.tiagosansao.lifetools"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
